{
  "name": "thefrosty/wp-utilities",
  "description": "A library containing my standard development resources",
  "version": "1.0.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Austin Passy",
      "email": "thefrosty@users.noreply.github.com",
